{
  "name": "gatsby-ecommerce-theme",
  "version": "1.0.0",
  "private": true,
  "description": "An e-commerce theme designed by Matter Design for Netlify.",
  "author": "Matter Design",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop -p 5000",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,json,css,md,yml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,css,md,yml}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "prepare": "husky install",
    "plop": "plop",
    "test": "jest",
    "update:styles": "node scripts/updateSustainableStyle.js"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@types/styled-components": "^5.1.34",
    "cloudinary": "^2.7.0",
    "co-body": "^6.2.0",
    "dotenv": "^16.6.1",
    "encoding": "^0.1.13",
    "formidable": "^3.5.4",
    "fs-extra": "^11.3.0",
    "gatsby": "^5.14.5",
    "gatsby-adapter-netlify": "^1.2.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-postcss": "^6.14.0",
    "gatsby-plugin-react-helmet": "^6.14.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-json": "^5.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "micro": "^10.0.1",
    "mini-css-extract-plugin": "^2.9.2",
    "node-fetch": "^2.6.1",
    "nodemailer": "^7.0.3",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-slick": "^0.30.3",
    "square": "^43.0.0",
    "styled-components": "^6.1.19",
    "webpack": "^5.99.9"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/eslint-parser": "^7.27.5",
    "@eslint/js": "^9.29.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "babel-jest": "^30.0.2",
    "eslint": "^7.32.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "lint-staged": "^16.1.2",
    "plop": "^4.0.1",
    "prettier": "^3.2.5",
    "react-router-dom": "^7.6.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md,yml}": [
      "prettier --write"
    ]
  }
}
